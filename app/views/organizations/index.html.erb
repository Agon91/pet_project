<h1>Organization#index</h1>
<p>Find me in app/views/pages/home.html.erb</p>


<div class="card" style="width: 18rem;">
    <% @organizations.each do |organization| %>
    <%#= cl_image_tag organization.photo.key class="card-img-top" %>
    <div class="card-body">
    <h5 class="card-title"><%= link_to organization.name, organization_path(organization) %></h5>
    <p class="card-text"><%= organization.description %></p>
    <%#= link_to organization.name, organization_path(organization)  class="btn btn-primary" %>
</div>
<% end %>
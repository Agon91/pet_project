<%= form_with(url: organization_admins_path, method: "post") do %>
  <%= label_tag(:email, "User Email:") %>
  <%= email_field_tag(:email) %>
  <br/>
  <%= check_box_tag(:can_edit) %>
  <%= label_tag(:can_edit, "Can Edit") %>
 <br/>
  <%= check_box_tag(:can_add_events) %>
  <%= label_tag(:can_edit, "Can Add Events") %>
 <br/>
  <%= check_box_tag(:can_add_posts) %>
  <%= label_tag(:can_edit, "Can Add Posts") %>
 <br/>
  <%= check_box_tag(:can_add_admins) %>
  <%= label_tag(:can_edit, "Can Add Admins") %>
 <br/>
  <%= submit_tag("Save") %> 
<% end %>
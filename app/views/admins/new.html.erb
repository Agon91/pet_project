<%= simple_form_for @admin do |f| %>
  <%= f.input :email, prepend: "example-", append: "@gmail.com" %>
  <%= f.input :can_edit, as: :check_boxes %>
  <%= f.input :can_add_events, as: :check_boxes %>
  <%= f.input :can_add_posts, as: :check_boxes %>
  <%= f.input :can_add_admins, as: :check_boxes %>  
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>